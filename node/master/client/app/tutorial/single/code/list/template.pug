h5 CODE LIST PAGE

div.list
   div(ng-repeat="code in vm.codes")
      h6 {{ code.title }}
      app-code(bind="code.body", language="javascript")
      div.buttons(style="margin-top: 10px;")
         button(ng-click="vm.runCode(code)") RUN CODE

