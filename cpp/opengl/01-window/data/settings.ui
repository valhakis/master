<interface>
  <object id="window1" class="GtkWindow">
    <property name="visible">True</property>
    <property name="title">Settings Window</property>
    <property name="border-width">5</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="visible">True</property>
        <property name="row-spacing">5</property>
        <property name="column-spacing">5</property>
        <!-- ROW 1 -->
        <child>
          <object class="GtkLabel">
            <property name="visible">True</property>
            <property name="label">Application</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="triangleCheckBtn" class="GtkCheckButton">
            <property name="visible">True</property>
            <property name="label">Triangle</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Application</property>
          </object>
          <packing>
            <property name="left-attach">2</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <!-- ROW 2 -->
        <child>
          <object class="GtkLabel">
            <property name="visible">True</property>
            <property name="label">Application</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
        <child>
          <object id="rectangleCheckBtn" class="GtkCheckButton">
            <property name="visible">True</property>
            <property name="label">Rectangle</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Application</property>
          </object>
          <packing>
            <property name="left-attach">2</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
