<interface>
  <object id="window1" class="GtkWindow">
    <property name="visible">True</property>
    <property name="title">Window 3</property>
    <property name="border-width">5</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="visible">True</property>
        <property name="row-spacing">5</property>
        <property name="column-spacing">5</property>
        <child>
          <object id="quitBtn" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Quit</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
        <child>
          <object id="settingsBtn" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Settings</property>
          </object>
          <packing>
            <property name="left-attach">2</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="sample" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Application</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="textview" class="GtkTextView">
            <property name="visible">True</property>
            <property name="editable">True</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="startServerBtn" class="GtkButton">
            <property name="visible">True</property>
            <property name="label">Start Server</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
